{
  "manifest_version": 2,

  "name": "eBay Profit Calculator",
  "description": "This extension allows you to calculate the profit from an eBay sale.",
  "version": "1.40",
  "permissions": [
    "activeTab"
  ],
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png",
             "256": "icon256.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "eBay Profit Calculator"
  },
  "background": {
    "scripts": ["jquery-3.1.1.min.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}