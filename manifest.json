{
  "manifest_version": 2,

  "name": "eBay Profit Calculator",
  "description": "This extension allows you to calculate the profit from an eBay sale.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-3.1.1.min.js", "popup.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}